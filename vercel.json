{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    { "source": "/miniapp", "destination": "/miniapp" },
    { "source": "/embed", "destination": "/embed" }
  ],
  "env": {
    "NEYNAR_API_KEY": "@neynar-api-key",
    "SIGNER_UUID": "@signer-uuid",
    "REDIS_URL": "@redis-url",
    "REDIS_TOKEN": "@redis-token",
    "NEXT_PUBLIC_BASE_URL": "https://unit-game-ruddy.vercel.app"
  }
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "autoDeploy": true
  }
}
